{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "eslint.config.js",
    "build.config.ts",
    "build.publish.ts",
    "addons/**/*.ts",
    "src/main.ts",
    "src/utils/libs/**/*.ts"
  ],
  "project": [
    "eslint.config.js",
    "build.config.ts",
    "build.publish.ts",
    "addons/**/*.ts",
    "src/**/*.ts"
  ],
  "ignoreExportsUsedInFile": true,
  "ignore": ["tests-runtime/", "dist-jsr/", "dist-npm/", "dist-libs/"],
  "ignoreBinaries": ["code", "db:push"],
  "ignoreDependencies": ["octokit", "ora", "querystring"]
}
